<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Archivo+Black|Archivo+Narrow" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/css/weather-icons.min.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navigation">
            @Html.ActionLink("NPWS", "Index", "Home", new { area = "" }, new { @class = "nav-link navbar-brand" })
            @Html.ActionLink("Top Parks", "FavoriteParks", "FavoriteParks", new { area="" }, new { @class = "nav-link" })
            @Html.ActionLink("Survey", "Survey", "FavoriteParks", new { area = "" }, new { @class = "nav-link" })
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    @*<script src="~/Scripts/bootstrap.min.js"></script>*@
    
</body>
</html>